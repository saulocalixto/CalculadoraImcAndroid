<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.imc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".addUsuario"
            android:parentActivityName=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.addUsuario" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".dashboard" android:parentActivityName=".MainActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
            <intent-filter>
                <action android:name="android.intent.action.dashboard" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".relatorio"
            android:parentActivityName=".dashboard"
            android:label="RelatÃ³rio" android:theme="@style/relatorio">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".dashboard" />
            <intent-filter>
                <action android:name="android.intent.action.relatorio"></action>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".dieta"
            android:label="Dietas" android:theme="@style/dieta">
            <intent-filter>
                <action android:name="android.intent.action.dieta"></action>
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity><!-- ATTENTION: This was auto-generated to add Google Play services to your project for
